{
  "name": "emily-js",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "ts-node-workaround": "tsc --showConfig > tsconfig.ts-node.json",
    "start": "cp .env dist; cd dist && node -r dotenv/config main.js",
    "dev": "ts-node-dev --transpile-only -r dotenv/config --project tsconfig.ts-node.json src/main.ts",
    "build": "rm -rf dist; tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.3",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "^20.4.4",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^3.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
